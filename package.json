  "description": "代码生成器",
  "scripts": {
    "web:preview": "vite preview",
    "web:dev": "vite serve ./react",
    "web:build": "tsc -p ./react && vite build ./react",
    "server:dev": "tsc -p ./server --watch",
    "start:server": "node ./server/bin/www",
    "lint": "eslint ./{react,server}/src/**/*{.jsx,.js,.ts,.tsx}",
    "lint:fix": "eslint ./{react,server}/src/**/*{.jsx,.js,.ts,.tsx} --fix"
  },
