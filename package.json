{
  "name": "arkgen",
  "version": "1.0.0",
  "description": "代码生成器",
  "scripts": {
    "web:preview": "vite preview",
    "web:dev": "vite serve ./react",
    "web:build": "tsc -p ./react && vite build ./react",
    "server:dev": "tsc -p ./server --watch",
    "server:build": "tsc -p ./server",
    "start:server": "hotnode ./server/bin/www",
    "start:web": "npm run web:dev",
    "lint": "eslint ./{react,server}/src/**/*{.jsx,.js,.ts,.tsx}",
    "lint:fix": "eslint ./{react,server}/src/**/*{.jsx,.js,.ts,.tsx} --fix"
  },
  "bin": {
    "arkgen": "index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArchyInk/code-generator.git"
  },
  "author": "archy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ArchyInk/code-generator/issues"
  },
  "homepage": "https://github.com/ArchyInk/code-generator#readme",
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "fs-extra": "^10.0.0",
    "morgan": "~1.9.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@types/express": "^4.17.13",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@vitejs/plugin-react": "^1.0.7",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.8.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2"
  }
}
